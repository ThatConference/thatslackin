    "node": "^8"
  },
  "now": {
    "name": "that-slackin",
    "alias": "thatslack.thatconference.com",
    "env": {
      "PORT": "443"
    },
    "scale": {
      "sfo1": {
        "min": 1,
        "max": 1
      }
    }
