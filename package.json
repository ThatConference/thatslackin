{
  "name": "thatslackin",
  "version": "1.0.2",
  "description": "That Conference THAT slackin WebSite",
  "main": "server.js",
  "scripts": {
    "test": "echo \"who needs tests, really ðŸ˜¢ \" && exit 0",
